@font-face
  font-family: 'vote-icon'
  src:  url('../fonts/vote-icon.eot?xrjogg')
  src:  url('../fonts/vote-icon.eot?xrjogg#iefix') format('embedded-opentype'),
    url('../fonts/vote-icon.ttf?xrjogg') format('truetype'),
    url('../fonts/vote-icon.woff?xrjogg') format('woff'),
    url('../fonts/vote-icon.svg?xrjogg#vote-icon') format('svg')
  font-weight: normal
  font-style: normal


[class^="icon-"], [class*=" icon-"]
  /* use !important to prevent issues with browser extensions that change fonts */
  font-family: 'vote-icon' !important
  speak: none
  font-style: normal
  font-weight: normal
  font-variant: normal
  text-transform: none
  line-height: 1

  /* Better Font Rendering =========== */
  -webkit-font-smoothing: antialiased
  -moz-osx-font-smoothing: grayscale


.icon-star_gray:before
  content: "\e924"
  color: #dbdbdb

.icon-five_star:before
  content: "\e91e"

.icon-vote:before
  content: "\e920"

.icon-favorites:before
  content: "\e905"

.icon-notebook:before
  content: "\e913"

.icon-praise:before
  content: "\e914"

